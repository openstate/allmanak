  2.3.1 31-03-2011
  Het veld 'zetel' in 'zetels' is optioneel gemaakt zodat wanneer 'zetel' leeg is de xml toch valideert
  
  2.3.2 28-09-2017
  Het veld 'categorie' is toegevoegd aan 'gemeente' en 'organisatie' 
  
  2.3.3 20-10-2017
  verhoging lengte 'naam' van 'organisatie' naar 1024
  
  2.4.0 21-11-2017
  toevoegen gemeenschappelijke regelingen, zie oo-export-regeling-2.4.0.xsd 
  
  2.4.1 21-02-2018
  bevoegdheidsattributie is aangepast naar bevoegdheidsverkrijging en is nu multivalue
  
  2.4.2 05-04-2018
  telefoon, emailadres, webadres kunnen een label hebben
  
  2.4.3 31-05-2018
  ministeries, zelfstandigeBestuursorganen toevoegen, splitsen op basis van organisatieTypen
  
  2.4.4 01-08-2018
  classificatie toevoegen
  
  2.4.5 01-09-2018
  organisatietype afleidbaar gemaakt uit type
  SiSacode vervangen met organisatiecode
  
  2.4.6 01-10-2018
  classificatie grondslag
  
  2.4.7 10-01-2019
  bevoegdheden kan meer tekens bevatten
  
  2.4.8 01-02-2019
  meerdere resourceIdentifiers mogelijk, plus versienummer toegevoegd

  2.4.9 01-04-2019
  contactEmail is verwijderd
  bestuursorganen toegevoegd
  bronhouder aangepast
  
  2.4.10 01-06-2019
  meerdere organisatietypes mogelijk per organisatie
  
  2.4.11 01-08-2019
  locatie toegevoegd aan adres
  format verdeelsleutel, aantalFTE, oppervlakte, wateroppervlakte
  
  2.4.12 15-10-2019
  evaluatierapport bij zbo meerdere waardes mogelijk
  
  2.4.13 15-01-2020
  vorige versie toegevoegd aan zbo, gemeenschappelijke regeling
  categorie verwijderd
  
  2.4.14 30-01-2020
  volgende versie toegevoegd aan zbo, gemeenschappelijke regeling
  huisletter is in het veld huisnummer opgegaan
  
  2.4.15 01-03-2020
  grensoverschrijdende gemeenschappelijke regelingen toegevoegd
  
  2.4.16 15-04-2020
  wettelijke voorschrift gewijzigd in wettelijke grondslag
  meeerdere bevoegdheden mogelijk met optioneel artikel
  
  2.4.17 10-11-2020
  lengte bevoegdheid onbeperkt
  
  2.4.18 01-10-2020
  optioneel rechtsvorm, relatie met ministerie, kaderwet adviescollege van toepassing en instellingsbesluiten toegevoegd aan organisatieType
  
  2.4.19 15-11-2020
  optioneel wettelijke grondslag en afwijkende bepaling toegevoegd aan organisatieType
  optioneel coordinaten (lon, lat, rijksdriehoeksstelsel x en y) toegevoegd aan adres
  
  2.4.20 15-12-2020
  optioneel personeelsomvang bij organisatie (ivm organisatietype agentschap)
  optioneel laatste doorlichting bij organisatie (ivm organisatietype agentschap)
  optioneel taken en bevoegdheden bij organisatie (ivm organisatietype agentschap)
  optioneel uitzonderingen/actieve afwijkingen van de Regeling agentschappen (ivm organisatietype agentschap)
  optioneel datum van publiceren begroting bij zelfstandige bestuursorganen
  
  2.4.21 15-06-2021
  datum ter verificatie en datum laatste mutatie toegevoegd
  
  2.4.22 28-06-2021
  subnaam toegevoegd aan zbo
  
  2.4.23 06-10-2021
  informatie wob-verzoek toegevoegd
  
  2.5.0 15-01-2022
  nieuwe naamgeving: organisaties.overheid.nl ipv almanak.overheid.nl
  
  2.6.0 20-06-2022
  geografie
  raad
  partijlidmaatschap medewerker
  gemeenteType komt te vervallen
  kaderwet adviescollege/zbo
  evaluatieverslagen
  taken & bevoegdheden
  wettelijke grondslagen
  begroting
  personeel
  afwijking regeling
  doorlichting
  bevoegdheden
  beleidsterreinen
  zelfstandige bestuursorganen zelfde structuur als organisaties
  deelnemende organisaties
  identificatiecodes
  metadata
  regeling type
  gemeenschappelijke regeling/regionaal samenwerkingsorgaan
  
  2.6.1 15-11-2022
  contact 
  adres
  bevoegdheidsverkrijging

  2.6.2 03-04-2023
  contact
  woo-informatie

  2.6.3 03-05-2023
  woo-index
 -->
<xs:schema xmlns="https://organisaties.overheid.nl/static/schema/oo/export/2.6.3" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="https://organisaties.overheid.nl/static/schema/oo/export/2.6.3" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:attribute name="timestamp" type="xs:string"/>
  <xs:element name="overheidsorganisaties">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="organisaties" minOccurs="0"/>
        <xs:element ref="gemeenschappelijkeRegelingen" minOccurs="0"/>
        <xs:element ref="gemeenschappelijkeRegelingenGrensoverschrijdend" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="timestamp"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="organisaties">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="organisatie"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="gemeenschappelijkeRegelingen">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="regeling"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="gemeenschappelijkeRegelingenGrensoverschrijdend">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="regeling"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:attribute name="naam">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:element name="adressen">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="adres" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="postAdres">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="adres"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="bezoekAdres">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="adres"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="adres">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="type" minOccurs="0"/>
        <xs:element ref="toelichting" minOccurs="0"/>
        <xs:element ref="straat" minOccurs="0"/>
        <xs:element ref="huisnummer" minOccurs="0"/>
        <xs:element name="toevoeging" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="250"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="postbus" minOccurs="0"/>
        <xs:element ref="postcode" minOccurs="0"/>
        <xs:element ref="plaats" minOccurs="0"/>
        <xs:element ref="regio" minOccurs="0"/>
        <xs:element ref="provincieAfkorting" minOccurs="0"/>
        <xs:element ref="land" minOccurs="0"/>
        <xs:element ref="centroide" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="postcode">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="50"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="postbus">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="50"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="huisnummer">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="50"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="straat">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="plaats">
    <xs:simpleType>
      <xs:restriction base="plaatsType">
        <xs:minLength value="1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="naam">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="1024"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="subnaam">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="type">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:attribute name="version" type="versionType"/>
  <xs:simpleType name="versionType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="25"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="resourceIdentifierTypeContent">
    <xs:restriction base="xs:string">
      <xs:maxLength value="250"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="resourceIdentifierType">
    <xs:simpleContent>
      <xs:extension base="resourceIdentifierTypeContent">
        <xs:attribute ref="naam" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:element name="resourceIdentifier" type="resourceIdentifierType"/>
  <xs:element name="ictuCode">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ictuCode" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="afkorting">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="50"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="startDatum" type="xs:date"/>
  <xs:element name="eindDatum" type="xs:date"/>
  <xs:element name="datumInwerkingtreding" type="xs:date"/>
  <xs:element name="datumUitwerkingtreding" type="xs:date"/>
  <xs:element name="toetredingsDatum" type="xs:date"/>
  <xs:element name="datumTerVerificatie" type="xs:date"/>
  <xs:element name="datumMutatie" type="xs:date"/>
  <xs:element name="geografie">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="oppervlakte" type="oppervlakteType" minOccurs="0"/>
        <xs:element name="wateroppervlakte" type="wateroppervlakteType" minOccurs="0"/>
        <xs:element ref="aantalInwoners" minOccurs="0"/>
        <xs:element ref="bevatPlaatsen" minOccurs="0"/>
        <xs:element name="inwoners" type="inwonersType" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="wooInformatie">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="urls" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="wooIndex">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="documentLocatie" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="woo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="wooInformatie" minOccurs="0"/>
        <xs:element ref="wooIndex" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="documentLocatie">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="informatiecategorie" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="url" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="toelichting" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="informatiecategorie">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="2048"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="overzichtURL">
    <xs:simpleType>
      <xs:restriction base="xs:anyURI">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="urls">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="url" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="overzichtURL" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:attribute name="eenheid" type="eenheidType"/>
  <xs:simpleType name="eenheidType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="7"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="oppervlakteTypeContent">
    <xs:restriction base="xs:token">
      <xs:pattern value="([0-9]*(,[0-9]*)?)"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="oppervlakteType">
    <xs:simpleContent>
      <xs:extension base="oppervlakteTypeContent">
        <xs:attribute ref="eenheid" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:simpleType name="wateroppervlakteContent">
    <xs:restriction base="xs:token">
      <xs:pattern value="([0-9]*(,[0-9]*)?)"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="wateroppervlakteType">
    <xs:simpleContent>
      <xs:extension base="wateroppervlakteContent">
        <xs:attribute ref="eenheid" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="inwonersType">
    <xs:simpleContent>
      <xs:extension base="xs:nonNegativeInteger">
        <xs:attribute ref="eenheid" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:element name="aantalInwoners" type="xs:nonNegativeInteger"/>
  <xs:simpleType name="plaatsType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="1024"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="bevatPlaatsen" type="plaatsType"/>
  <xs:element name="raad">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="totaalZetels" minOccurs="0"/>
        <xs:element name="partijen">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="partij" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="totaalZetels" type="xs:nonNegativeInteger"/>
  <xs:element name="aantalZetels" type="xs:nonNegativeInteger"/>
  <xs:element name="partij">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="naam"/>
        <xs:element ref="aantalZetels"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="beleidsterrein">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element ref="naam" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute ref="resourceIdentifier"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="subtype">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="soortAdviescollege">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="soortZbo">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="locatie">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="telex">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="50"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="fax">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="50"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="installatie" type="xs:string"/>
  <xs:element name="afbeelding">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="bron" minOccurs="0"/>
        <xs:element ref="bronvermelding" minOccurs="0"/>
        <xs:element ref="beschrijving" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="beschrijving" type="xs:string"/>
  <xs:element name="doel" type="xs:string"/>
  <xs:element name="vergoeding" type="xs:nonNegativeInteger"/>
  <xs:element name="standplaats">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="partijFunctie">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="partijLidmaatschap">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="aantal" type="xs:nonNegativeInteger"/>
  <xs:element name="p" type="xs:string"/>
  <xs:element name="adresString" type="xs:string"/>
  <xs:element name="contact">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="telefoonnummers" type="telefoonnummersType" minOccurs="0"/>
        <xs:element name="emailadressen" type="emailadressenType" minOccurs="0"/>
        <xs:element ref="fax" minOccurs="0"/>
        <xs:element ref="telex" minOccurs="0"/>
        <xs:element ref="bezoekAdres" minOccurs="0"/>
        <xs:element ref="postAdres" minOccurs="0"/>
        <xs:element name="internetadressen" type="internetadressenType" minOccurs="0"/>
        <xs:element name="socialMedia" type="socialmediaType" minOccurs="0"/>
        <xs:element name="contactpaginas" type="contactpaginasType" minOccurs="0"/>
        <xs:element ref="beschrijving" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="deelnemendeOrganisatie">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element ref="naam" minOccurs="0"/>
        <xs:element ref="toetredingsDatum" minOccurs="0"/>
        <xs:element ref="verdeelsleutel" minOccurs="0"/>
        <xs:element ref="bestuursorganen" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="systeemId"/>
      <xs:attribute ref="resourceIdentifierTOOI"/>
      <xs:attribute ref="resourceIdentifierOWMS"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="functie">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element ref="naam"/>
        <xs:element ref="type" minOccurs="0"/>
        <xs:element ref="subtype" minOccurs="0"/>
        <xs:element name="medewerkers" type="medewerkersType" minOccurs="0"/>
        <xs:element ref="ictuCode" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="systeemId"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="medewerker">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element ref="naam"/>
        <xs:element ref="afbeelding" minOccurs="0"/>
        <xs:element ref="partijLidmaatschap" minOccurs="0"/>
        <xs:element ref="partijFunctie" minOccurs="0"/>
        <xs:element ref="standplaats" minOccurs="0"/>
        <xs:element ref="installatie" minOccurs="0"/>
        <xs:element ref="adressen" minOccurs="0"/>
        <xs:element ref="contact" minOccurs="0"/>
        <xs:element ref="startDatum" minOccurs="0"/>
        <xs:element ref="eindDatum" minOccurs="0"/>
        <xs:element ref="vergoeding" minOccurs="0"/>
        <xs:element ref="ictuCode" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="systeemId"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="provincieAfkorting">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:maxLength value="250"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="organisatie" type="organisatieType"/>
  <xs:element name="regeling" type="regelingType"/>
  <xs:element name="bronvermelding" type="xs:string"/>
  <xs:element name="bron" type="xs:anyURI"/>
  <xs:complexType name="organisatieType" mixed="true">
    <xs:sequence>
      <xs:element ref="naam"/>
      <xs:element ref="subnaam" minOccurs="0"/>
      <xs:element ref="afkorting" minOccurs="0"/>
      <xs:element name="types">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="type" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element ref="subtype" minOccurs="0"/>
      <xs:element ref="soortAdviescollege" minOccurs="0"/>
      <xs:element ref="soortZbo" minOccurs="0"/>
